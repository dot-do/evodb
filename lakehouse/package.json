{
  "name": "@evodb/lakehouse",
  "version": "0.1.0-rc.1",
  "description": "Iceberg-inspired manifest and R2 storage for EvoDB lakehouse",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "evodb",
    "lakehouse",
    "iceberg",
    "manifest",
    "r2",
    "cloudflare",
    "workers",
    "time-travel",
    "snapshots"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "lakehouse"
  },
  "dependencies": {
    "@evodb/core": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260120.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "sideEffects": false
}
