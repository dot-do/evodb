{
  "name": "@evodb/edge-cache",
  "version": "0.1.0-rc.1",
  "description": "Edge cache integration for EvoDB with cdn.workers.do and 5GB file support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./prefetch": {
      "types": "./dist/prefetch.d.ts",
      "import": "./dist/prefetch.js"
    },
    "./planner": {
      "types": "./dist/cache-aware-planner.d.ts",
      "import": "./dist/cache-aware-planner.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "evodb",
    "cloudflare",
    "workers",
    "edge-cache",
    "cdn"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "edge-cache"
  },
  "dependencies": {
    "@evodb/core": "workspace:*"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260120.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
